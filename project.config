[project]
	description = SoftTech project
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/carrot13
[access "refs/for/refs/*"]
	read = group user/carrot13
	push = group user/carrot13
	label-Code-Review = -2..+2 group user/carrot13
	label-Verified = -1..+1 group user/carrot13
	rebase = group user/carrot13
	removeReviewer = group user/carrot13
	submit = group user/carrot13
[access "refs/heads/*"]
	read = group user/carrot13
	create = group user/carrot13
	push = +force group user/carrot13
	pushMerge = group user/carrot13
[access "refs/tags/*"]
	push = group user/carrot13
	push = +force group user/carrot13
